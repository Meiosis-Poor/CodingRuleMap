{
  "name": "modernize",
  "description": null,
  "rules": [
    "AvoidBind",
    "AvoidCArrays",
    "ConcatNestedNamespaces",
    "DeprecatedHeaders",
    "DeprecatedIosBaseAliases",
    "LoopConvert",
    "MacroToEnum",
    "MakeShared",
    "MakeSmartPtr",
    "MakeUnique",
    "MinMaxUseInitializerList",
    "PassByValue",
    "RawStringLiteral",
    "RedundantVoidArg",
    "ReplaceAutoPtr",
    "ReplaceDisallowCopyAndAssignMacro",
    "ReplaceRandomShuffle",
    "ReturnBracedInitList",
    "ShrinkToFit",
    "TypeTraits",
    "UnaryStaticAssert",
    "UseAuto",
    "UseBoolLiterals",
    "UseConstraints",
    "UseDefaultMemberInit",
    "UseDesignatedInitializers",
    "UseEmplace",
    "UseEqualsDefault",
    "UseEqualsDelete",
    "UseIntegerSignComparison",
    "UseNodiscard",
    "UseNoexcept",
    "UseNullptr",
    "UseOverride",
    "UseRanges",
    "UseScopedLock",
    "UseStartsEndsWith",
    "UseStdFormat",
    "UseStdNumbers",
    "UseStdPrint",
    "UseTrailingReturnType",
    "UseTransparentFunctors",
    "UseUncaughtExceptions",
    "UseUsing"
  ]
}