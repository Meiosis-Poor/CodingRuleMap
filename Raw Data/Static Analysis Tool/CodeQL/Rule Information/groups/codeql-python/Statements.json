{
  "name": "Statements",
  "description": null,
  "rules": [
    "AssertLiteralConstant",
    "AssertOnTuple",
    "BreakOrReturnInFinally",
    "C_StyleParentheses",
    "ConstantInConditional",
    "DocStrings",
    "ExecUsed",
    "IterableStringOrSequence",
    "MismatchInMultipleAssignment",
    "ModificationOfLocals",
    "NestedLoopsSameVariable",
    "NestedLoopsSameVariableWithReuse",
    "NonIteratorInForLoop",
    "RedundantAssignment",
    "ReturnOrYieldOutsideFunction",
    "ShouldUseWithStatement",
    "SideEffectInAssert",
    "StatementNoEffect",
    "StringConcatenationInLoop",
    "TopLevelPrint",
    "UnnecessaryDelete",
    "UnnecessaryElseClause",
    "UnnecessaryPass",
    "UnreachableCode",
    "UnusedExceptionObject",
    "UseOfExit"
  ]
}